#include <stdio.h>

void times_table(int num1, int num2){
	int a, b;
	for(a = num1; a<=num2; a++){
		for(b = 1; b<10; b++){
			printf("%dX%d=%d\n", a, b, a*b);
		}
	}
}
int main(void){
	int num1 = 0;
	int num2 = 0;
	printf("정수 입력: ");
	scanf("%d %d", &num1, &num2);
	if(num1<num2) times_table(num1,num2);
	else times_table(num2,num1);
	return 0;
}
